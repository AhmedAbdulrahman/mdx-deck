{
  "name": "mdx-deck",
  "version": "2.3.2",
  "description": "MDX-based presentation decks",
  "bin": {
    "mdx-deck": "./cli.js"
  },
  "main": "index.js",
  "scripts": {
    "start": "./cli.js demo.mdx -p 8080",
    "build": "./cli.js build demo.mdx",
    "eject": "./cli.js eject demo.mdx",
    "help": "./cli.js"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "repository": "github:jxnblk/mdx-deck",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@mdx-deck/components": "^2.3.2",
    "@mdx-deck/gatsby-theme": "^2.3.2",
    "@mdx-deck/layouts": "^2.3.2",
    "@mdx-deck/themes": "^2.3.2",
    "@mdx-js/mdx": "^1.0.1",
    "@mdx-js/react": "^1.0.1",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.0",
    "emotion-theming": "^10.0.7",
    "execa": "^1.0.0",
    "find-up": "^3.0.0",
    "gatsby": "^2.4.2",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "gatsby-plugin-emotion": "^4.0.6",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-source-filesystem": "^2.0.33",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "meow": "^5.0.0",
    "pkg-conf": "^3.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.1",
    "remark-emoji": "^2.0.2",
    "remark-unwrap-images": "^0.2.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  }
}
